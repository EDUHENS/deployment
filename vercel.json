{
  "buildCommand": "cd frontend && npm install && npm run build && cd ../backend && npm install",
  "github": {
    "silent": true
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/**.js": {
      "runtime": "nodejs20.x",
      "maxDuration": 30
    }
  }
}
